---
import type { CollectionEntry } from "astro:content";
import FormattedDate from "./FormattedDate.astro";
interface Props {
    conference: CollectionEntry<'conferences'>;
}
const { conference } = Astro.props;

---
{ conference.data.conference }.
{conference.data.location},
{conference.data.meetingdate}.
<em>{conference.data.title}</em>
{conference.data.type != undefined ? `[${conference.data.type}]` : ""}
